{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "User Registration Schema",
    "required": [],
    "properties": {
        "password": {
            "type": "string",
            "minLength": 5,
            "maxLength": 20
        },
        "firstName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "lastName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "img": {
            "type": "string"
        },
        "email": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60,
            "format": "email"
        }
    }
}