{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "User Login Schema",
    "required": [
        "username",
        "password"
    ],
    "properties": {
        "username": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "password": {
            "type": "string",
            "minLength": 5,
            "maxLength": 20
        }
    },
    "examples": [{
        "username": "EsmaErdem",
        "password": "password123"
    }]
}